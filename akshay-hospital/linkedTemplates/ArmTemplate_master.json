{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "akshay-hospital"
		},
		"AzureDataLakeStorage1_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_ls'"
		},
		"AzureDatabricksDeltaLake_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake_ls'"
		},
		"hosa_sql_linkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hosa_sql_linkedService'"
		},
		"AzureDataLakeStorage1_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ak0adls0dev0sa.dfs.core.windows.net/"
		},
		"hosa_sql_linkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "akshay-sqlserver.database.windows.net"
		},
		"hosa_sql_linkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hosa_sql_linkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "akshay"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "akshay-hospital_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_ls_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_ls_accountKey')]"
					},
					"AzureDatabricksDeltaLake_ls_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake_ls_accessToken')]"
					},
					"hosa_sql_linkedService_password": {
						"value": "[parameters('hosa_sql_linkedService_password')]"
					},
					"AzureDataLakeStorage1_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_ls_properties_typeProperties_url')]"
					},
					"hosa_sql_linkedService_properties_typeProperties_server": {
						"value": "[parameters('hosa_sql_linkedService_properties_typeProperties_server')]"
					},
					"hosa_sql_linkedService_properties_typeProperties_database": {
						"value": "[parameters('hosa_sql_linkedService_properties_typeProperties_database')]"
					},
					"hosa_sql_linkedService_properties_typeProperties_userName": {
						"value": "[parameters('hosa_sql_linkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}